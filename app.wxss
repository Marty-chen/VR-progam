/**app.wxss**/
/* 全局颜色初始化 */
page,view,text,swiper,swiper-item,image,navigator{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
page {
  /* 定义主题颜色 */
  --themeColor: #CE0000;
  font-size: 32rpx;
  color: #333;
  --priceColor: #F2510F;
}
image {
  width: 100%;
}

.bdb {
  border-bottom: 16rpx solid #f7f7f7;
}
.cfbtn {
  width: 686rpx;
  height: 78rpx;
  background-color: var(--themeColor);
  border-radius: 40rpx;
  color: #fff;
}
.ilb {
  display: inline-block;
}